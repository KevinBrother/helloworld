# API配置
VITE_API_BASE_URL=http://localhost:3001
VITE_API_PREFIX=api
VITE_API_VERSION=v1

# WebSocket配置
VITE_WS_URL=ws://localhost:3002
VITE_WS_PATH=/ws

# 应用配置
VITE_APP_NAME=Universal Spider
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=智能爬虫工具

# 环境配置
VITE_NODE_ENV=development
VITE_BUILD_MODE=development

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_PWA=false

# 上传配置
VITE_UPLOAD_MAX_SIZE=52428800
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif,image/webp,video/mp4,video/webm

# 地图配置 (如果需要)
VITE_MAP_API_KEY=
VITE_MAP_PROVIDER=google

# 分析配置 (可选)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# 主题配置
VITE_DEFAULT_THEME=light
VITE_THEME_COLOR=#3B82F6

# 国际化配置
VITE_DEFAULT_LOCALE=zh-CN
VITE_FALLBACK_LOCALE=en-US

# 缓存配置
VITE_CACHE_VERSION=1.0.0
VITE_CACHE_ENABLED=true

# 调试配置
VITE_DEBUG_ENABLED=true
VITE_LOG_LEVEL=info

# CDN配置 (生产环境)
VITE_CDN_URL=
VITE_STATIC_URL=

# 第三方服务
VITE_RECAPTCHA_SITE_KEY=
VITE_STRIPE_PUBLIC_KEY=