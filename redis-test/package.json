{
  "name": "redis-bullmq-example",
  "version": "1.0.0",
  "description": "Redis and BullMQ usage examples with TypeScript, pnpm, and vitest",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "queue": "tsx src/bullmq/queue.ts",
    "worker": "tsx src/bullmq/worker.ts",
    "worker:handle": "tsx src/bullmq/hand-worker.ts",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "node dist/index.js"
  },
  "keywords": [
    "redis",
    "bullmq",
    "typescript",
    "vitest",
    "pnpm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "tsx": "^4.6.0"
  },
  "dependencies": {
    "redis": "^4.6.0",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.0"
  }
}